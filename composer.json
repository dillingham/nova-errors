{
    "name": "dillingham/nova-errors",
    "description": "Display errors at the top of your forms",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "NovaErrorField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaErrorField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
